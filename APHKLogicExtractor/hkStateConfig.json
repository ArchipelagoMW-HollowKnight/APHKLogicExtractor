{
  "BeneficialModifiers": [
    "$BENCHRESET",
    "$FLOWERGET",
    "$HOTSPRINGRESET",
    "$REGAINSOUL",
    "$STARTRESPAWN" // restores soul and HP
  ],
  "DetrimentalModifiers": [
    "$SHADESKIP",
    "$SPENDSOUL",
    "$TAKEDAMAGE",
    "$LIFEBLOOD",
    "$EQUIPPEDCHARM",
    "$STAGSTATEMODIFIER"
  ],
  "OtherModifiers": [
    "$CASTSPELL",
    "$SHRIEKPOGO",
    "$SLOPEBALL",
    "$WARPTOBENCH", // savequitreset + benchreset
    "$WARPTOSTART", // savequitreset + startrespawn
    "$SAVEQUITRESET" // spends all soul & "breaks" flower, but also restores health and shade
  ],
  "ArgumentClassifiers": [
    // spell casts are always bad if you can't get soul before/after them
    {
      "Prefix": "$CASTSPELL",
      "Comparison": "NoArgStartsWith",
      "Test": "before:",
      "ClassificationWhenMatched": "Detrimental"
    },
    {
      "Prefix": "$CASTSPELL",
      "Comparison": "NoArgStartsWith",
      "Test": "after:",
      "ClassificationWhenMatched": "Detrimental"
    },
    {
      "Prefix": "$SHRIEKPOGO",
      "Comparison": "NoArgStartsWith",
      "Test": "before:",
      "ClassificationWhenMatched": "Detrimental"
    },
    {
      "Prefix": "$SHRIEKPOGO",
      "Comparison": "NoArgStartsWith",
      "Test": "after:",
      "ClassificationWhenMatched": "Detrimental"
    },
    {
      "Prefix": "$SLOPEBALL",
      "Comparison": "NoArgStartsWith",
      "Test": "before:",
      "ClassificationWhenMatched": "Detrimental"
    },
    {
      "Prefix": "$SLOPEBALL",
      "Comparison": "NoArgStartsWith",
      "Test": "after:",
      "ClassificationWhenMatched": "Detrimental"
    }
  ]
}